{
    "name": "sae-manager/composer",
    "description": "SAE Manager - Gestion des SAE",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "phpmailer/phpmailer": "^7.0",
        "ext-json":  "*"
    },
    "autoload": {
        "psr-4": {
            "Controllers\\": "src/Controllers/",
            "Models\\": "src/Models/",
            "Views\\": "src/Views/",
            "Shared\\":  "src/Shared/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unitaire\\":  "tests/Unitaire/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    }
}